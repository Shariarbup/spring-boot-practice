<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	
	<bean id="vehicle1" class="com.bjit.vehicle.Vehicle">
		<property name="name" value="Car"></property>
	</bean>
	<bean id="vehicle2" class="com.bjit.vehicle.Vehicle">
		<property name="name" value="Bike"></property>
	</bean>
	<bean id="vehicle3" class="com.bjit.vehicle.Vehicle">
		<property name="name" value="AutoRickshaw"></property>
	</bean>
	
	
	<bean id="engine1" class="com.bjit.engine.Engine">
		<property name="engineType" value="Combustion Engine"></property>
	</bean>
	<bean id="engine2" class="com.bjit.engine.Engine">
		<property name="engineType" value="Electrical Engine"></property>
	</bean>
	

</beans>
